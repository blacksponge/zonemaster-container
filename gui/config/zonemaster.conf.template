server {
    listen       80;
    listen  [::]:80;
    server_name  localhost;

    #access_log  /var/log/nginx/host.access.log  main;

    root   /usr/share/nginx/html;

    location / {
        try_files $uri /index.html =404;
    }

    location /api {
        proxy_pass ${ZONEMASTER_API_ENDPOINT};
    }
}
